{
    "openapi": "3.0.0",
    "info": {
        "title": "Mon API Laravel",
        "description": "Documentation de l'API avec Swagger dans Laravel",
        "version": "1.0.0"
    },
    "paths": {
        "/api/login": {
            "post": {
                "tags": [
                    "Authentification"
                ],
                "summary": "Connexion de l'utilisateur",
                "operationId": "3673edffcdc18c06ac5aef5e2219d774",
                "requestBody": {
                    "required": true,
                    "content": {
                        "application/json": {
                            "schema": {
                                "required": [
                                    "email",
                                    "password"
                                ],
                                "properties": {
                                    "email": {
                                        "type": "string",
                                        "format": "email",
                                        "example": "test@example.com"
                                    },
                                    "password": {
                                        "type": "string",
                                        "format": "password",
                                        "example": "secret"
                                    }
                                },
                                "type": "object"
                            }
                        }
                    }
                },
                "responses": {
                    "200": {
                        "description": "Connexion réussie",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "properties": {
                                        "user": {
                                            "type": "object"
                                        },
                                        "token": {
                                            "type": "string",
                                            "example": "1|XyzTokenString"
                                        }
                                    },
                                    "type": "object"
                                }
                            }
                        }
                    },
                    "401": {
                        "description": "Identifiants invalides",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "properties": {
                                        "message": {
                                            "type": "string",
                                            "example": "Identifiants invalides"
                                        }
                                    },
                                    "type": "object"
                                }
                            }
                        }
                    }
                }
            }
        },
        "api/logout": {
            "post": {
                "tags": [
                    "Authentification"
                ],
                "summary": "Déconnexion de l'utilisateur",
                "operationId": "38c43a1f9291ef028204b69a67d1a1a3",
                "responses": {
                    "200": {
                        "description": "Déconnexion réussie",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "properties": {
                                        "message": {
                                            "type": "string",
                                            "example": "Déconnexion réussie"
                                        }
                                    },
                                    "type": "object"
                                }
                            }
                        }
                    },
                    "401": {
                        "description": "Non authentifié"
                    }
                },
                "security": [
                    {
                        "sanctum": []
                    }
                ]
            }
        },
        "/api/register": {
            "post": {
                "tags": [
                    "Authentification"
                ],
                "summary": "Enregistrer un nouvel utilisateur",
                "operationId": "7d8c5938c0f8c4cee857ce8e9a12d028",
                "requestBody": {
                    "required": true,
                    "content": {
                        "application/json": {
                            "schema": {
                                "required": [
                                    "name",
                                    "email",
                                    "password"
                                ],
                                "properties": {
                                    "name": {
                                        "type": "string",
                                        "example": "Jean Dupont"
                                    },
                                    "email": {
                                        "type": "string",
                                        "format": "email",
                                        "example": "jean@example.com"
                                    },
                                    "password": {
                                        "type": "string",
                                        "format": "password",
                                        "example": "secret123"
                                    }
                                },
                                "type": "object"
                            }
                        }
                    }
                },
                "responses": {
                    "200": {
                        "description": "Utilisateur enregistré avec succès",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "properties": {
                                        "user": {
                                            "description": "Détails de l'utilisateur",
                                            "type": "object"
                                        },
                                        "token": {
                                            "type": "string",
                                            "example": "1|XyzTokenString"
                                        }
                                    },
                                    "type": "object"
                                }
                            }
                        }
                    },
                    "422": {
                        "description": "Erreur de validation",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "properties": {
                                        "message": {
                                            "type": "string",
                                            "example": "The email has already been taken."
                                        }
                                    },
                                    "type": "object"
                                }
                            }
                        }
                    }
                }
            }
        },
        "/api/import-notes": {
            "post": {
                "tags": [
                    "Examens"
                ],
                "summary": "Importer les notes depuis un fichier Excel",
                "description": "Charge un fichier Excel de correction, extrait numero_inscri + note, met à jour la base de données et retourne la liste finale des étudiants.",
                "operationId": "048f4aa75118c2ed207105b97fd4325a",
                "requestBody": {
                    "required": true,
                    "content": {
                        "multipart/form-data": {
                            "schema": {
                                "required": [
                                    "file"
                                ],
                                "properties": {
                                    "file": {
                                        "description": "Fichier Excel (.xlsx ou .xls) contenant les numéros d’inscription et les notes",
                                        "type": "string",
                                        "format": "binary"
                                    }
                                },
                                "type": "object"
                            }
                        }
                    }
                },
                "responses": {
                    "200": {
                        "description": "Liste des étudiants avec leurs notes",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "type": "array",
                                    "items": {
                                        "properties": {
                                            "id": {
                                                "type": "integer",
                                                "example": 1
                                            },
                                            "cin": {
                                                "type": "string",
                                                "example": "12345678"
                                            },
                                            "numero_inscri": {
                                                "type": "string",
                                                "example": "20231234"
                                            },
                                            "first_name": {
                                                "type": "string",
                                                "example": "Ali"
                                            },
                                            "last_name": {
                                                "type": "string",
                                                "example": "Ben Salah"
                                            },
                                            "exam": {
                                                "type": "string",
                                                "example": "Maths"
                                            },
                                            "exam_date": {
                                                "type": "string",
                                                "format": "date",
                                                "example": "2025-06-01"
                                            },
                                            "qr_hash": {
                                                "type": "string",
                                                "example": "a1b2c3d4e5"
                                            },
                                            "qr_path": {
                                                "type": "string",
                                                "example": "storage/qrcodes/1234.png"
                                            },
                                            "note": {
                                                "type": "number",
                                                "format": "float",
                                                "example": 14.75
                                            },
                                            "created_at": {
                                                "type": "string",
                                                "format": "date-time"
                                            },
                                            "updated_at": {
                                                "type": "string",
                                                "format": "date-time"
                                            }
                                        },
                                        "type": "object"
                                    }
                                }
                            }
                        }
                    },
                    "400": {
                        "description": "Requête invalide (fichier manquant ou mauvais format)"
                    }
                }
            }
        },
        "/api/upload-notes": {
            "post": {
                "tags": [
                    "Excel"
                ],
                "summary": "Importer les notes depuis un fichier Excel (numInscri 4 derniers + note)",
                "description": "Charge un fichier Excel contenant deux colonnes (numInscri (ou ses 4 derniers chiffres) et note). Met à jour la colonne `note` de la table students en recherchant l'étudiant par les 4 derniers chiffres de numero_inscri.",
                "operationId": "8ffba8f385ebafe03d10f219075b5f8d",
                "requestBody": {
                    "required": true,
                    "content": {
                        "multipart/form-data": {
                            "schema": {
                                "required": [
                                    "excel_file"
                                ],
                                "properties": {
                                    "excel_file": {
                                        "description": "Fichier Excel (.xlsx ou .xls) contenant deux colonnes: numero_inscri (ou sa fin 4 chiffres) et note",
                                        "type": "string",
                                        "format": "binary"
                                    }
                                },
                                "type": "object"
                            }
                        }
                    }
                },
                "responses": {
                    "200": {
                        "description": "Liste des résultats d'import (ok / not_found) pour chaque ligne",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "properties": {
                                        "results": {
                                            "type": "array",
                                            "items": {
                                                "properties": {
                                                    "numero_inscri": {
                                                        "type": "string"
                                                    },
                                                    "note": {
                                                        "type": "number",
                                                        "format": "float"
                                                    },
                                                    "status": {
                                                        "type": "string",
                                                        "example": "ok|not_found|invalid"
                                                    }
                                                },
                                                "type": "object"
                                            }
                                        }
                                    },
                                    "type": "object"
                                }
                            }
                        }
                    },
                    "400": {
                        "description": "Fichier manquant ou mauvais format"
                    },
                    "500": {
                        "description": "Erreur serveur"
                    }
                }
            }
        }
    },
    "tags": [
        {
            "name": "Authentification",
            "description": "Authentification"
        },
        {
            "name": "Examens",
            "description": "Examens"
        },
        {
            "name": "Excel",
            "description": "Excel"
        }
    ]
}